{
    "buildCommand": "npm run build",
    "framework": "nextjs",
    "env": {
        "NEXT_PUBLIC_API_URL": "@next_public_api_url",
        "NEXT_PUBLIC_HEDERA_NETWORK": "testnet"
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://your-backend-url.com/api/:path*"
        }
    ]
}